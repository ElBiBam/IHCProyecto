<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fullihc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".bottonnav"
            android:label="@string/title_activity_bottonnav"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".nav"
            android:parentActivityName=".MainActivity"></activity>

        <activity
            android:name=".login"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".navigation"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".preferencias"
            android:parentActivityName=".navigation"></activity>
        <activity
            android:name=".actividad"
            android:parentActivityName=".navigation"></activity>
        <activity
            android:name=".supermercado"
            android:parentActivityName=".navigation"></activity>
    </application>

</manifest>